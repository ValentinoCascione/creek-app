<!DOCTYPE html>
<html>
  <title>CreekApp</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div class="container">
    <% if user_signed_in? %>
    <%= render 'layouts/header_co' %>
    <% else %>
    <%= render 'layouts/header_deco' %>
    <% end %>
    <%= yield %>
  </div>
  </body>
  
   <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</html>
