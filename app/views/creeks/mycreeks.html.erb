<h1>My Creeks</h1>

<% @creeks.each do |creek| %>
    <p><%= creek.title %></p>
    <%= link_to "Show my creek", mycreek_path(creek) %>
    <%= link_to "Edit my creek", edit_creek_path(creek) %>
    <%= link_to "Delete my creek", creek_path(creek), method: :delete %>
<% end %>

<%= link_to "Back", '/' %>
<%= link_to "Add a new hosting creek", new_creek_path %>
